{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "brtled/taxonomy-nav",
  "version": "0.1.0",
  "title": "分类法导航",
  "description": "显示一个多级分类法导航菜单",
  "category": "theme",
  "textdomain": "brtled",
  "icon": "editor-ul",
  "attributes": {
    "rel": {
      "type": "string",
      "attribute": "rel",
      "default": ""
    },
    "linkTarget": {
      "type": "string",
      "default": "_self"
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "html": false,
    "color": {
      "gradients": true,
      "link": true
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "script": "file:./script.js"
}
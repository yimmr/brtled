{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "brtled/product-meta",
  "version": "0.1.0",
  "title": "产品元数据",
  "description": "关于产品的一些数据",
  "category": "theme",
  "textdomain": "brtled",
  "icon": "editor-ul",
  "usesContext": [
    "postId",
    "postType",
    "queryId"
  ],
  "attributes": {
    "textAlign": {
      "type": "string"
    }
  },
  "supports": {
    "html": false,
    "color": {
      "gradients": true,
      "link": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}
{
	"$schema": "https://schemas.wp.org/wp/6.1/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#222",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#3398e5",
					"name": "主色",
					"slug": "primary"
				},
				{
					"color": "#182142",
					"name": "副色",
					"slug": "secondary"
				},
				{
					"color": "#F6F6F6",
					"name": "三色",
					"slug": "tertiary"
				},
				{
					"color": "#3aa8fc",
					"name": "高亮",
					"slug": "light-primary"
				},
				{
					"color": "#eee",
					"name": "淡灰",
					"slug": "gray-1"
				},
				{
					"color": "#ccc",
					"name": "淡灰2",
					"slug": "gray-2"
				},
				{
					"color": "#aaa",
					"name": "淡灰3",
					"slug": "gray-3"
				},
				{
					"color": "#808080",
					"name": "灰色",
					"slug": "gray"
				},
				{
					"color": "#666",
					"name": "灰色2",
					"slug": "gray-6"
				}
			]
		},
		"custom": {
			"section-padding": {
				"bottom": "var(--wp--preset--spacing--30)",
				"top": "var(--wp--preset--spacing--30)"
			}
		},
		"layout": {
			"contentSize": "840px",
			"wideSize": "1200px"
		},
		"spacing": {
			"spacingSizes": [
				{
					"name": "1",
					"size": "clamp(1.5rem, 5vw, 2rem)",
					"slug": "30"
				},
				{
					"name": "2",
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "40"
				},
				{
					"name": "3",
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "50"
				},
				{
					"name": "4",
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "60"
				},
				{
					"name": "5",
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "70"
				},
				{
					"name": "6",
					"size": "clamp(7rem, 14vw, 11rem)",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontSizes": [
				{
					"fluid": {
						"max": "0.8125rem",
						"min": "0.0625rem"
					},
					"size": "0.75rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "0.9625rem",
						"min": "0.91rem"
					},
					"size": "0.91rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "0.9625rem"
					},
					"size": "1rem",
					"slug": "large"
				},
				{
					"fluid": false,
					"size": "2rem",
					"slug": "x-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/code": {
				"typography": {
					"fontSize": "var:preset|font-size|x-large",
					"textTransform": "capitalize"
				}
			}
		},
		"color": {
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--light-primary)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem",
			"padding": {
				"bottom": "0",
				"left": "1rem",
				"right": "1rem",
				"top": "0"
			}
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"area": "uncategorized",
			"name": "about-card",
			"title": "About Card"
		},
		{
			"area": "header",
			"name": "header",
			"title": "页眉"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "页脚"
		}
	],
	"customTemplates": [
		{
			"name": "wp-custom-template-company",
			"title": "Company",
			"postTypes": [
				"page"
			]
		},
		{
			"name": "wp-custom-template-contact",
			"title": "Contact",
			"postTypes": [
				"page"
			]
		},
		{
			"name": "wp-custom-template-solution",
			"title": "Solution",
			"postTypes": [
				"page"
			]
		},
		{
			"name": "product",
			"title": "Product summary",
			"postTypes": [
				"page"
			]
		},
		{
			"name": "single-post",
			"postTypes": [
				"post"
			],
			"title": "Article"
		},
		{
			"name": "single-product",
			"postTypes": [
				"product"
			],
			"title": "Product"
		}
	]
}